<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>AD_GetDeploymentById</name>
        <uuid>645ba993-dafc-4783-9ebc-2a5897024e91_GETONERECORD</uuid>
        <description>Returns the Deployment that matches the passed id</description>
        <parentUuid>9fe7a67d-221f-4480-8baf-ce89637400ed</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{urn:com:appian:types:AD}AD_Deployment',
  #"SYSTEM_SYSRULES_queryEntity"(
    entity: #"72485958-b51b-4820-a154-20fab2039f73_MAINDSECONSTANT",
    query: #"SYSTEM_SYSRULES_query"(
      filter: #"SYSTEM_SYSRULES_queryFilter"(
        field: "id",
        operator: "=",
        value: ri!id
      ),
      pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(1, 1)
    )
  ).data
)</definition>
        <namedTypedValue>
            <name>id</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
